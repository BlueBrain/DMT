<html>
  <body>
    <h1>Validation against data</h1>

    <img src="$validation_image_path" alt="apologies.png"/>
    <br>
    <p>$caption</p>

    <h1>Datasets</h1>

    <ul>
      #for $k, $d in $validation_datasets.iteritems()
      <li><a href="$d.url">[$d.label]</a> -$d.citation<br>
        <p> <i>$d.what</i> </p>
      </li>
      #end for
    </ul>

    <p>
      #if $is_pass:
      <font color="00FF00">PASSED</font>
      #elif $is_inconclusive:
      <font color="0000FF">INCONCLUSIVE</font>
      #else
      <font color="#FF0000">FAILED</font>
      #end if
      <br>
      p value: p=$p_value
    </p>

  </body>
</html>
