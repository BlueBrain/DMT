<html>
  <body>

    <h1>$phenomenon Validation</h1>
    <h2>$author_name</h2>
    <h3>$author_affiliation</h3>
    <br>
    <img src="$validation_image__path" alt="apologies.png"/>
    <br>
    <p>$caption</p>

    <h1>Datasets</h1>

    <ul>
      #for $k, $d in $datasets.iteritems()
      <li><a href="$d.url">[$d.short_name]</a> -$d.citation<br>
        <p><i><$d.what></i></p>
      </li>
      #end for
    </ul>

    <h1>Status</h1>
    <p>
      #if $is_pass:
      <font color="#00FF00">PASSED</font>
      #else if $is_fail:
      <font color="#FF0000">FAILED</font>
      #else
      <font color="0000FF">INCONCLUSIVE></font>
      #end if
      <br>
      p-value: p=$p_value
    </p>

  </body>
</html>
